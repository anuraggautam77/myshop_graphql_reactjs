{
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "globals": {
      "$": true,
      "jQuery": true,
      "window": true,
      "siteConfigProps": true,
      "eventListingSSIJson": true,
      "includeSSIJson": true,
      "Handlebars": true,
      "LoadJS": true,
      "BreakpointModule": true,
      "WindowScrollModule": true,
      "SessionStorageModule": true,
      "CommonModule": true,
      "DTMTrackingModule": true,
      "EventModule": true,
      "UtilityModule": true,
      "sinon": true,
      "it": true,
      "expect": true,
      "assert": true,
      "self": true,
      "elm": true,
      "before": true,
      "beforeEach": true,
      "afterEach": true,
      "describe": true,
      "picturefill": true,
      "modalvideo": true,
      "chai": true,
      "videojs": true,
      "CarouselModule": true,
      "ALAPP": true,
      "RichRewardsCommonVariables": true,
      "rrProfileAddress": true,
      "RichRewardsComponent": true,
      "RewardsInlineFormComponent": true,
      "LTYFramework": true,
      "codeText": true,
      "_carouselCard": true,
      "UTIL": true,
      "_displayCatalogItems": true,
      "_filteringLabels": true,
      "_satellite": true,
      "_modalBoxTemplate": true,
      "_mailingAddressTemplate": true,
      "globalDcsUri": true,
      "DataView": true,
      "Promise": true
    },
    "rules": {
      "semi": ["error", "always"],
      "strict": "error",
      "quotes": ["error", "single"],
      "brace-style": ["error", "stroustrup"],
      "eqeqeq": "error",
      "no-eq-null": "error",
      "default-case": "error",
      "arrow-parens": ["error", "as-needed"],
      "no-eval": "warn",
      "array-bracket-spacing": "warn",
      "comma-spacing": "error",
      "no-inline-comments": "error",
      "indent": ["error", 2],
      "no-empty-function": "error",
      "no-trailing-spaces": "error",
      "space-before-function-paren": 0,
      "spaced-comment": "warn",
      "curly": "error",
      "no-lonely-if": "error",
      "eol-last": ["warn", "always"],
      "no-shadow": [
        "error",
        {
          "builtinGlobals": false,
          "hoist": "functions",
          "allow": []
        }
      ],
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ],
      "comma-dangle": ["error", "always"],
      "newline-before-return": "error",
      "key-spacing": "error",
      "keyword-spacing": "error",
      "no-undef": [
        "warn",
        {
          "typeof": true
        }
      ],
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "after-used",
          "ignoreRestSiblings": false
        }
      ]
    }
  }
  